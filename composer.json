{
    "name": "lidai/laravel-redirects",
    "version": "1.0.3",
    "description": "Nested URLs redirect logic for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "redirect",
        "url",
        "slug",
        "seo"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Liam Aird",
            "email": "lidai@jfmedier.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lidai\\Redirects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lidai\\Redirects\\Tests\\": "Tests"
        },
        "classmap": [
            "Tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lidai\\Redirects\\ServiceProvider"
            ]
        }
    }
}
